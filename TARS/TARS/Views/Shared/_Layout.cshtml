<!DOCTYPE html>

<html>
	<head>
        <meta charset="utf-8" />
		<meta name="description" content="Idaho Department of Health and Welfare Time, Accounting, and Reporting system">
		<meta name="keywords" content="TARS" />
		<title>@ViewBag.Title</title>
		<link href="@Url.Content("~/Content/template.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
	</head>
	<body>
		<div class="container">
			<div id="top">	
					
				<img src="@Url.Content("~/Content/themes/base/images/logo.png")"  id="logo" alt="logo" />
				<table class="nav">
					<tr>
						<td>Administration</TD>
						<td>Managerial</td>
						<td>User Panel</td>
						<td>Logout</td>
					</tr>
				</table>
				<table class="contextMenu">
					<tr>
						<td><a href="User/">View Timesheet</a></TD>
						<td>Locate Work Effort</td>
					</tr>
				</table>
				<table class="update">
					<tr>
						<td><i>Current Pay Period: </i></td><td><b><font color="ff0000">10/31 - 11/5</font></b></td>
					</tr>
					<tr>
						<td><i>TimeSheet Status:</i></td><td><font color="green"><b>Approved</b></font></td>
					</tr>
				</table>
			</div>

			<div id = "contents">
               <ul id="menu">
                    <li>@Html.Partial("_LogOnPartial")</li>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
				@RenderBody()
			</div>
		</div>

	</body>
</html>
